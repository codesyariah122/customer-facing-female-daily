{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": ["./src/app/*"],
      "@app": ["./src/app"],
      "@assets/*": ["./src/assets/*"],
      "@assets": ["./src/assets"],
      "@components/*": ["./src/components/*"],
      "@components": ["./src/components"],
      "@constants/*": ["./src/constants/*"],
      "@constants": ["./src/constants"],
      "@contexts/*": ["./src/context/*"],
      "@contexts": ["./src/context"],
      "@dummydata/*": ["./src/data/*"],
      "@graphql-ctcd/*": ["./src/graphql/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@hooks": ["./src/hooks"],
      "@layouts/*": ["./src/layouts/*"],
      "@services/*": ["./src/services/*"],
      "@store/*": ["./src/store/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
